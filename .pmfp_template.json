{
    "language": "go",
    "env": "gomod",
    "description": "go语言grpc服务和sdk的组件库.",
    "author": "hsz",
    "template_type": "components",
    "requires": [
        "github.com/Golang-Tools/loggerhelper@v0.0.3",
        "github.com/Golang-Tools/schema-entry-go@v0.0.4",
        "github.com/golang/protobuf@v1.5.2",
        "github.com/json-iterator/go@v1.1.11",
        "github.com/liyue201/grpc-lb@v0.0.0-20201117062843-867c3e20933f",
        "google.golang.org/grpc@v1.38.0",
        "google.golang.org/protobuf@v1.26.0"
    ],
    "template_keys": {
        "module_name": {
            "description": "定义模块的名字",
            "default": "{{ project_name }}",
            "ask": true
        }
    },
    "components": {
        "simple_module": {
            "source": "module/module.go.jinja",
            "description": "简单模块模板",
            "default_path": "{{ module_name }}/{{ module_name }}.go"
        },
        "plugin_module": {
            "source": "plugin/plugin_module.go.jinja",
            "description": "插件模块模板",
            "default_path": "plugins/{{ module_name }}.go"
        },
        "plugin_module_usage": {
            "source": "plugin/plugin_main.go.jinja",
            "description": "插件模块使用例子模板",
            "default_path": "plugintest/main.go"
        }
    }
}